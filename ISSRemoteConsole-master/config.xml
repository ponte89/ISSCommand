<?xml version='1.0' encoding='utf-8'?>
<widget id="it.unibo.iss.group11.command" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RemoteConsoleRobot</name>
    <description>
        A remote console for ISS project of a Robot
    </description>
    <author >
        Lorenzo Pontellini
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <gap:plugin name="com.kumbe.phonegap.zoom.zoomcontrol" version="0.1.7" />
    <gap:plugin name="Accelerometer" value="org.apache.cordova.core.AccelListener" />
	<gap:plugin name="Compass" value="org.apache.cordova.core.CompassListener" />
	<gap:plugin name="Geolocation" value="org.apache.cordova.core.GeoBroker" />
</widget>
